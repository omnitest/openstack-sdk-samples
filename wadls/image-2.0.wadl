                         required="true"
                         path="$.parameters.path">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The location within the image where the operation is to be performed.</para>
                    </wadl:doc>
                  </param>
                  <param name="value"
                         style="plain"
                         type="xsd:string"
                         required="false"
                         path="$.parameters.value">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The actual value to be added or replaced. It is not required for the <code>delete</code>
                            operation.</para>
                    </wadl:doc>
                  </param>
               </representation>
            </request>
            <response status="200">
               <doc title="Success">Request succeeded.</doc>
               <representation mediaType="application/json">
                  <doc xml:lang="EN">
                    <xsdxt:sample>
                        <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                           <programlisting xmlns="http://docbook.org/ns/docbook" language="javascript">{
   "id":"e7db3b45-8db7-47ad-8109-3fb55c2c24fd",
   "name":"Fedora 17",
   "status":"queued",
   "visibility":"public",
   "tags": ["fedora", "beefy"],
   "created_at":"2012-08-11T17:15:52Z",
